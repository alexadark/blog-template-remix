    "@remix-run/serve": "^2.6.0",
    "@storyblok/react": "^3.0.8",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "isbot": "^4.4.0",
    "lucide-react": "^0.321.0",
    "markdown-to-jsx": "^7.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "storyblok-rich-text-react-renderer": "^2.9.1"
