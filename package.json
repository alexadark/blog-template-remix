{
  "name": "blog-template-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "platform": "node",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
